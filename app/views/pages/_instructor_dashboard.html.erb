<h1>MY INSTRUCTOR PROFILE</h1>
  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Upcoming</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Past</a>
    </li>
  </ul>

  <div class="tab-content w-100" id="myTabContent">
  <div class="tab-pane fade show active w-100" id="home" role="tabpanel" aria-labelledby="home-tab">
    <% if current_user.bookings.upcoming.each == 0 %>
        <p> No bookings </p>
    <% else %>
      <div class="d-flex flex-wrap justify-content-between mt-5 w-100">
        <% current_user.bookings.upcoming.each do |booking| %>
            <%= render "shared/instructor_group_cards", booking: booking %>
        <% end %>
      </div>
    <% end %>
  </div>

          
      <% if current_user.client_bookings.upcoming.each == 0 %>
          <p> No bookings </p>
      <% else %>
        <%# <div class="d-flex flex-wrap justify-content-between mt-5 w-100">
          <% current_user.client_bookings.upcoming.each do |instructor_booking| %>
            <%# <%= render "shared/instructor_card", booking: booking %>
          <%# <% end %>
        <%# </div> %> 
      <% end %>
    </div>

    <div class="tab-pane fade w-100" id="profile" role="tabpanel" aria-labelledby="profile-tab">
      <% if current_user.client_bookings.past.each == 0 %>
        <p> No bookings </p>
      <% else %>
        <div class="d-flex flex-wrap justify-content-between mt-5 w-100">
          <% current_user.bookings.past.each do |instructor_booking| %>
            <%= instructor_booking.status %>
            <% instructor_booking.user_bookings.each do |user_booking|%>
              <%= user_booking.user.first_name%>
              <%= user_booking.user.last_name%>
              <%= user_booking.user.phone_number%>
              <%= user_booking.user.category%>
              <%= user_booking.user.technical_skill%>
              <%= user_booking.participants_number%>
            <% end %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>